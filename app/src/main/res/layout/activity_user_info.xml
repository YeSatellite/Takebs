<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yesat.takebs.UserInfoActivity">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/pro_ava"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark_normal"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/tv_rate"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tv_rate"
        app:layout_constraintHorizontal_bias="0.58" />

    <View
        android:layout_height="1dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="0dp"
        android:id="@+id/view3"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/comm" />

    <TextView
        android:text="Country :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textSize="18sp"
        android:textColor="@color/common_google_signin_btn_text_light_focused"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintLeft_toLeftOf="@+id/textView17" />

    <TextView
        android:text="DOB :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textSize="18sp"
        android:textColor="@color/common_google_signin_btn_text_light_focused"
        app:layout_constraintLeft_toLeftOf="@+id/textView17"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:text="City :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textSize="18sp"
        android:textColor="@color/common_google_signin_btn_text_light_focused"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintLeft_toLeftOf="@+id/textView17" />

    <TextView
        android:id="@+id/pro_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="64dp"
        android:layout_marginStart="64dp"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView17"
        app:layout_constraintLeft_toRightOf="@+id/textView17" />

    <TextView
        android:id="@+id/pro_dob"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintLeft_toLeftOf="@+id/pro_email" />

    <TextView
        android:id="@+id/pro_coun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintLeft_toLeftOf="@+id/pro_dob" />

    <TextView
        android:id="@+id/pro_city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView13"
        app:layout_constraintLeft_toLeftOf="@+id/pro_coun" />

    <TextView
        android:text="Email :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:textSize="18sp"
        android:textColor="@color/common_google_signin_btn_text_light_focused"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/view3"
        android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/tv_rate"
        android:layout_width="22dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="4/5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pro_ava"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/comm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_rate"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/tv_rate"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/tv_rate" />

</android.support.constraint.ConstraintLayout>
